{
  "name": "jeyroik/extas-installer-import",
  "description": "Allow to export/import defined extas-compatible entities",
  "require": {
    "jeyroik/extas-installer": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vlucas/phpdotenv": "^3.4",
    "jeyroik/extas-repositories-mongo": "1.*",
    "jeyroik/extas-snuffbox": "0.*"
  },
  "autoload": {
    "psr-4": {
      "extas\\": "src/",
      "tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --coverage-clover=coverage.xml --whitelist=src tests"
  },
  "bin": [
    "src/bin/extas"
  ]
}